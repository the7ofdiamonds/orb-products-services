{
    "name": "orb-services/api",
    "description": "API to access services, invoices, receipts and make payments",
    "type": "project",
    "authors": [
        {
            "name": "THE7OFDIAMONDS.TECH"
        }
    ],
    "require": {
        "stripe/stripe-php": "^10.15",
        "vlucas/phpdotenv": "^5.5",
        "symfony/dotenv": "^6.3",
        "php": "^8.1",
        "phpmailer/phpmailer": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "ORB_Services\\": "",
            "Stripe\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stripe\\": [
                "tests/",
                "tests/Stripe/"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}